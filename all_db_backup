  `options` longtext,
  `scheduled` varchar(64) NOT NULL DEFAULT '',
  `registered_on` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',
  `friendly_name` varchar(64) NOT NULL DEFAULT '',
  `exported` bigint(20) NOT NULL DEFAULT '0',
